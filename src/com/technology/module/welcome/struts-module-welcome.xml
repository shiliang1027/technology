<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    
    <package name="technology-module-welcome" namespace="/" extends="website_struts">
    	<action name="welcomeAction" class="welcomeAction">
    	    <result name="index">/module/Index.jsp</result>
    	    <result name="logined" type="redirect">/main.html</result>
    	    <result name="logout" type="redirect">/index.html</result>
    	    <result name="main">/module/Main.jsp</result>
    	</action>
    </package>
</struts>
